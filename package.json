{
  "name": "lsp-view-tree",
  "version": "1.0.0",
  "description": "Language Server Protocol implementation for .view.tree files",
  "main": "lib/server.js",
  "scripts": {
    "build": "tsc -p .",
    "build:clean": "npm run clean && npm run build",
    "clean": "rm -rf lib/",
    "watch": "tsc -w -p .",
    "start": "node lib/server.js",
    "dev": "npm run build && npm run start",
    "dev:watch": "concurrently \"npm run watch\" \"nodemon lib/server.js\"",
    "test": "node test-client.js",
    "test:simple": "node test-simple.js",
    "test:interactive": "node interactive-test.js",
    "test:manual": "echo \"Manual testing: Start server with 'npm start', then use your LSP client to connect to stdio\"",
    "lint": "tslint --project tsconfig.json",
    "lint:fix": "tslint --project tsconfig.json --fix",
    "prepare": "npm run build",
    "prepack": "npm run build:clean"
  },
  "keywords": [
    "lsp",
    "language-server",
    "view-tree",
    "mol"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "vscode-languageserver": "^9.0.1",
    "vscode-languageserver-textdocument": "^1.0.11",
    "vscode-uri": "^3.0.8",
    "source-map-js": "^1.0.2"
  },
  "devDependencies": {
    "@types/node": "^20.10.0",
    "typescript": "^5.3.0",
    "tslint": "^6.1.3",
    "concurrently": "^8.2.2",
    "nodemon": "^3.0.2"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}